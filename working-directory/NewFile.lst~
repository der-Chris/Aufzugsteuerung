                        54
                        55     ; Interrupt Address for Timer1
                        56     ; This Interrupt checks every 10ms if a Button has been pressed
                        57     ORG 01bh
                        58     LCALL timer1 ; try LCALL. If error use CALL instead
                        59     RETI
                        60
                        61     CSEG AT 020H
                        62     ;initialize interrupts
                        63     init_interrupts:
                        64     MOV TMOD, 00010001b ;Timer1: Gate1=0, C/T1 = 0, Mode1 = 01; Timer0: Gate0=0, C/T0 = 0, Mode0 = 01;
                        65     MOV TCON, 01010000b ; Timer1: TF1=0, TR1(enables Timer1)=1; Timer0: TF0=0, TR0(enables Timer0)=1;
                        66
                        67     ; EA(global Interrupt Enabled) = 1, X = dont care, X = dont care, ES(enables serial interrupt)=0,
                        68     ; ET1(enables timer interrupt1)=1, EX1(enables External Interrupt1)=0,ET0(enables timer interrupt0)=0
                        69     ; Enable Timer0 Interrupts just if it is needed
                        70     MOV IE, 10001001b
                        71     ; interrupt Priority is correct
                        72     MOV TL0, 0F9h ; 249 in Low von Timer0
                        73     MOV TH0, 09h ; 9 in High von Timer0
                        74     MOV TL1, 0F9h ; 249 in Low von Timer1
                        75     MOV TH1, 03h ; 19 in High von Timer1
                        76     RET
                        77
                        78
                        79     ;initializes the parameters
                        80     init:
                        81     MOV P1, 00h ; turn off Engines -> Engines are low active
                        82     MOV P3, 0ffh ; MOV P3, #0ffh ;turn off Display -> Display is low active
