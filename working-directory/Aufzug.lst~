Aufzug                                                                                                                  PAGE 1
                         1     ; Aufzugsteuerung mit 3 Stockwerken
                         2     ;
                         3     ; Portbelegung:
                         4     ; P0.0  Knöpfe  Außen   0
                         5     ;    1                                  1
                         6     ;    2                                  2
                         7     ;    3                  Innen   0
                         8     ;    4                                  1
                         9     ;    5                                  2
                        10     ;    6
                        11     ;    7                  Tür zu
                        12     ;
                        13     ; P1.0  Motor   Tür             Auf
                        14     ;    1                                  Zu
                        15     ;    2                  Aufzug  Hoch
                        16     ;    3                                  Runter
                        17     ;    4  Licht   Knöpfe  0
                        18     ;    5                                  1
                        19     ;    6                                  2
                        20     ;    7  Gong
                        21     ;
                        22     ; P2.0  Sensor  Tür             Auf
                        23     ;    1                                  Zu
                        24     ;    2                  Druck
                        25     ;    3                  Lichtschranke
                        26     ;    4                  Stock   0
                        27     ;    5                                  1
                        28     ;    6                                  2
                        29     ;    7          -----
                        30     ;
                        31     ; P3    Display
                        32     ;
                        33     ; EX0   Notruf
                        34     ;
                        35     ; R0    Button 0 has been pressed
                        36     ; R1    Button 1 has been pressed
                        37     ; R2    Button 2 has been pressed
                        38     ; R7    Direction the Elevator is headding. R7 = 0 -> up, R7 = 1 -> down
                        39     CSEG AT 00H
                        40     LJMP init_interrupts
                        41
                        42     ; Interrupt Address for ExternalInterrupt0 -> Emergency Button
                        43     ORG 03h
                        44     LCALL ext0
                        45     RETI
                        46
                        47
                        48     ; Interrupt Address for Timer0
                        49     ; This Interrupt checks every 5ms if the Sensors are triggered
                        50     ORG 0bh
                        51     LCALL timer0_ISR
                        52     RETI
                        53
                        54
                        55     ; Interrupt Address for Timer1
                        56     ; This Interrupt checks every 10ms if a Button has been pressed
                        57     ORG 01bh
                        58     LCALL timer1_ISR
                        59     RETI
                        60
                        61     CSEG AT 0100H
                        62     ;initialize interrupts
                        63     init_interrupts:
                        64     MOV TMOD, 00010001b
                        65     ;Timer1: Gate1=0, C/T1 = 0, Mode1 = 01 ; Timer0: Gate0=0, C/T0 = 0, Mode0 = 01
                        66     MOV TCON, 01010000b
                        67     ; Timer1: TF1=0, TR1(enables Timer1)=1; Timer0: TF0=0, TR0(enables Timer0)=1
                        68
                        69     ; EA(global Interrupt Enabled) = 1, X = dont care, X = dont care, ES(enables serial interrupt)=0,
                        70     ; ET1(enables timer interrupt1)=1, EX1(enables External Interrupt1)=0,ET0(enables timer interrupt0)=0
                        71     ; Enable Timer0 Interrupts just if it is needed
                        72     MOV IE, 10001001b
                        73     ; interrupt Priority is correct
                        74     MOV TL0, 0F9h ; 249 in Low von Timer0
                        75     MOV TH0, 09h ; 9 in High von Timer0
                        76     MOV TL1, 0F9h ; 249 in Low von Timer1
                        77     MOV TH1, 03h ; 19 in High von Timer1
                        78     RET
                        79
                        80
                        81     ;initializes the parameters
                        82     init:
                        83     MOV P1, 00h ; turn off Engines -> Engines are low active
                        84     MOV P3, 0ffh ; MOV P3, #0ffh ;turn off Display -> Display is low active
                        85
                        86     MOV R0, 00h ; R0 = 1, if Button for First Floor has been pressed
                        87     MOV R1, 00h ; R1 = 1, if Button for Second Floor has been pressed
                        88     MOV R2, 00h ; R2 = 1, if Button for Third Floor has been pressed
                        89
                        90     LCALL initialCheckFloor
                        91     JMP goingDown
                        92
                        93
                        94
                        95     ; do something usefull
                        96     ext0:
                        97     MOV P3, 10000110b ;Activate LED´s for Error
                        98     RET
                        99
                       100
                       101     ; Check sensors, if sensors are triggered Or OpenDoor or SameFloor Buttons are pressed,open the doors
                       102     timer0_ISR:
                       103     MOV A, P3
                       104     ANL A, 00001100b ; If P3.2 or P3.3 is triggered, then Accu != 0
                       105     JZ checkDoorButtons ; And if Accu = 0, countinue
                       106     CLR P1.0
                       107     SETB P1.1 ; stop doors from closing, open doors
                       108     MOV A, P2
                       109     ANL A, 00000001b
                       110     JZ timer0 ; when doors are not closed, wait till they are
****ERROR:Labelistoofarfor8-bitrelativeaddressing.Trytodisablepeepholeoptimizationsiftheyareon.
                       111     ; If sensors are not triggered, check if a button from floor the elevator is in, is pressed
                       112     checkDoorButtons:
                       113     JB P2.4, checkFirstFloorDoors
                       114     JB P2.5, checkSecondFloorDoors
                       115     JB P2.6, checkThirdFloorDoors
                       116     ; Elevator has to be in one Floor
                       117     checkFirstFloorDoors:
                       118     MOV A, P0
                       119     ANL A, 00001001b
                       120     JZ initTimer0
                       121     closeDoorsFirstFloor:
                       122     MOV A, P2
                       123     ANL A, 00000001b
                       124     JZ closeDoorsFirstFloor ; when doors are not closed, wait till they are
                       125     JMP initTimer0
                       126     checkSecondFloorDoors:
                       127     MOV A, P0
                       128     ANL A, 00010010b
                       129     JZ initTimer0
                       130     closeDoorsSecondFloor:
                       131     MOV A, P2
                       132     ANL A, 00000001b
                       133     JZ closeDoorsSecondFloor ; when doors are not closed, wait till they are
                       134     JMP initTimer0
                       135     checkThirdFloorDoors:
                       136     MOV A, P0
                       137     ANL A, 00100100b
                       138     JZ initTimer0
                       139     closeDoorsThirdFloor:
                       140     MOV A, P2
                       141     ANL A, 00000001b
                       142     JZ closeDoorsThirdFloor ; when doors are not closed, wait till they are
                       143     JMP initTimer0
                       144     ; in the end, Timer 0 has to be reinitialized
                       145     initTimer0:
                       146     MOV TL0, 0F9h ; 249 in Low von Timer0
                       147     MOV TH0, 09h ; 9 in High von Timer0
                       148     RET
                       149
                       150
                       151     ;check if button is pressed
                       152     timer1_ISR:
                       153     MOV A, P0
                       154     ANL A, 00001001b ; If P0.0 or P0.3 is pressed, then Accu != 0
                       155     JZ checkSecondFloorButton ; And if Accu = 0, then check other floor
                       156     MOV R0, 01h
                       157     checkSecondFloorButton:
                       158     MOV A, P0
                       159     ANL A, 00010010b ; If P0.1 or P0.4 is pressed, then Accu != 0
                       160     JZ checkThirdFloorButton ; And if Accu = 0, then check other floor
                       161     MOV R1, 01h
                       162     checkThirdFloorButton:
                       163     MOV A, P0
                       164     ANL A, 00100100b ; If P0.2 or P0.5 is pressed, then Accu != 0
                       165     JZ initTimer1 ; And if Accu = 0, then check other floor
                       166     MOV R2, 01h
                       167     initTimer1:
                       168     ; in the end, Timer 1 has to be reinitialized
                       169     MOV TL1, 0F9h ; 249 in Low von Timer1
                       170     MOV TH1, 013h ; 19 in High von Timer1
                       171     RET
                       172
                       173
                       174     ;check in which floor the elevator is
                       175     initialCheckFloor:
                       176     JB P2.5, endInit
                       177     JB P2.6, endInit
                       178     JB P2.7, endInit
                       179     ; when we don’t know in which floor the elevator is, we wait for a button to be pressed
                       180     ; when a button is pressed we close the door and drive to the firstFloor
                       181     waitForButton:
                       182     ; check if a button is pressed
                       183     MOV A, P0
                       184     ANL A, 00111111b
                       185     JZ waitForButton ;jumps if Accu equals 0
                       186
                       187     ; dont check if closed or so, just to it
                       188     LCALL closeDoors
                       189     LCALL driveDown
                       190     LCALL openDoors
                       191     endInit:
                       192     RET
                       193
                       194
                       195     ;openDoor Routine
                       196     openDoors:
                       197     CLR P1.1
                       198     SETB P1.0
                       199     JNB P2.0, openDoors
                       200     CLR P1.0
                       201     RET
                       202
                       203
                       204     ;drive Down, till in first Floor
                       205     driveDown:
                       206     SETB P1.3
                       207     JNB P2.4, driveDown
                       208     CLR P1.3
                       209     RET
                       210
                       211
                       212     floorLogic:
                       213     ; implement floorLogic
                       214     CALL openDoors
                       215     ; here could be a timer
                       216     CALL closeDoors
                       217     ; Check if we are on top or at the bottom. If we are at the button, first try getting up.
                       218     CJNE R0, #00h, goingUp
                       219     CJNE R2, #00h, goingDown
                       220     CJNE R7, #00h, goingDown
                       221     JMP goingUp
                       222
                       223
                       224     ; Routine to Travel to FirstFloor
                       225     goToFirstFloor:
                       226     CLR P1.2
                       227     SETB P1.3
                       228     JNB P2.4, goToFirstFloor
                       229     CLR P1.3
                       230     LCALL showFirstFloor
                       231     JMP floorLogic
                       232
                       233     ; Routine to Travel to SecondFloor
                       234     goToSecondFloor:
                       235     JB P0.4, firstFloorToSecondFloor
                       236     JB P2.6, thirdFloorToSecondFloor
                       237     ; Elevator has to be in one Floor
                       238     firstFloorToSecondFloor:
                       239     CLR P1.3
                       240     SETB P1.2
                       241     JNB P2.5, goToFirstFloor
                       242     CLR P1.2
                       243     LCALL showSecondFloor
                       244     JMP floorLogic
                       245     thirdFloorToSecondFloor:
                       246     CLR P1.2
                       247     SETB P1.3
                       248     JNB P2.5, goToFirstFloor
                       249     CLR P1.3
                       250     LCALL showSecondFloor
                       251     JMP floorLogic
                       252
                       253
                       254     ; Routine to Travel to ThirdFloor
                       255     goToThirdFloor:
                       256     CLR P1.3
                       257     SETB P1.2
                       258     JNB P2.6, goToFirstFloor
                       259     CLR P1.2
                       260     LCALL showThirdFloor
                       261     JMP floorLogic
                       262
                       263
                       264     ; Routine to Close the Doors
                       265     closeDoors:
                       266     JB P2.1, dooresClosed
                       267     MOV A, IE
                       268     ORL A, 00000010b
                       269     MOV IE, A ; Activate interrupt to Check DoorSensors
                       270     CLR P1.0
                       271     SETB P1.1
                       272     JNB P2.1, closeDoors ; If Doores Are Closed STOP InterruptTimer and Return
                       273     dooresClosed:
                       274     MOV A, IE
                       275     ANL A, 11111101b
                       276     MOV IE, A ; Activate interrupt to Check DoorSensors
                       277     RET
                       278
                       279
                       280     goingDown:
                       281     MOV R7, 01h
                       282     MOV A,R1
                       283     JNZ goToSecondFloor
                       284     MOV A, R0
                       285     JNZ goToFirstFloor
                       286     ; if no button is pressed, check if button is pressed to drive up
                       287     JMP goingUP
                       288
                       289
                       290     goingUp:
                       291     MOV R7, 00h
                       292     MOV A,R1
                       293     JNZ goToSecondFloor
                       294     MOV A, R2
                       295     JNZ goToThirdFloor
                       296     ; if no button is pressed, check if button is pressed to drive down
                       297     JMP goingDown
                       298
                       299
                       300     showFirstFloor:
                       301     MOV P3, 11111001b ;Activate LED´s for first Floor
                       302     RET
                       303
                       304
                       305     showSecondFloor:
                       306     MOV P3, 10100100b ;Activate LED´s for second Floor
                       307     RET
                       308
                       309
                       310     showThirdFloor:
                       311     MOV P3, 10110000b ;Activate LED´s for third Floor
                       312     RET
                       313
                       314
                       315     END
